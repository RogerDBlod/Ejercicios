fun main(args: Array<String>) {

    var arreglo = arrayOf(
        intArrayOf(1, 2, 3, 4, 5, 6),
        intArrayOf(7, 8, 9, 10, 11, 12),
        intArrayOf(7, 8, 9, 10, 11, 12)

    )

    for (i in 0 until 4) {
        println("esta es la vuelta$i")
        for (j in 0 until 3) {
            println("esta es la vuelta del segundo bucle num $j")
            println(arreglo[j][i])
        }

    }
}
